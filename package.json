{
  "publisher": "fe92star",
  "name": "yapi-mock-server",
  "displayName": "yapi-mock-server",
  "version": "0.0.2",
  "packageManager": "pnpm@7.8.0",
  "description": "mock server for yapi application",
  "author": "fe92star <1737765939@qq.com> (https://github.com/fe92star/)",
  "license": "MIT",
  "homepage": "https://github.com/FE92star/yapi-mock-server#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/FE92star/yapi-mock-server"
  },
  "bugs": {
    "url": "https://github.com/FE92star/yapi-mock-server/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onCommand:extension.helloWorld"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "npm run build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "pnpm build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.17.2",
    "@types/node": "^18.6.4",
    "@types/vscode": "^1.69.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.21.0",
    "esno": "^0.16.3",
    "pnpm": "^7.8.0",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.21.0",
    "vsce": "^2.10.0"
  }
}
